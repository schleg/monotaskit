.form_wrapper
 %h2
  Log In
 - form_for @user_session, :url => user_session_path do |f|
  = f.error_messages
  %dl
   %dt
    = f.label :login, "User Name"
   %dd
    = f.text_field :login
   %dt
    = f.label :password
   %dd
    = f.password_field :password
   %div{:style=>"padding: .5em 0 0 0"}
    = f.check_box :remember_me
    = f.label :remember_me, "Remember Me"
  = f.submit "Log In"